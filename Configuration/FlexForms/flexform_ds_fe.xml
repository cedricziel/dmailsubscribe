<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.sheet_general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.additionalFields>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.additional_fields</label>
                            <config type="array">
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.additionalFields>
                    <settings.requiredFields>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.additional_fields</label>
                            <config type="array">
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.requiredFields>
                    <settings.muteConfrmationErrors>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.mute_confrmation_errors</label>
                            <config type="array">
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.muteConfrmationErrors>
                    <settings.muteUnsubscribeErrors>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.mute_unsubscribe_errors</label>
                            <config type="array">
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.muteUnsubscribeErrors>
                    <settings.lookupPids>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.lookup_pids</label>
                            <config type="array">
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>10</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.lookupPids>
                    <settings.categoryPids>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.category_pids</label>
                            <config type="array">
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>10</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.categoryPids>
                    <settings.pluginPageUid>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.plugin_page_uid</label>
                            <config type="array">
                                <type>input</type>
                                <eval>trim</eval>
                                <size>1</size>
                                <wizards type="array">
                                    <_PADDING type="integer">2</_PADDING>
                                    <link type="array">
                                        <type>popup</type>
                                        <title>Link</title>
                                        <icon>link_popup.gif</icon>
                                        <script>browse_links.php?mode=wizard</script>
                                        <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                                    </link>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.pluginPageUid>
                </el>
            </ROOT>
            <email>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.sheet_email</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.fromEmail>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.from_email</label>
                            <config type="array">
                                <type>input</type>
                                <size>30</size>
                                <eval>email,trim</eval>
                            </config>
                        </TCEforms>
                    </settings.fromEmail>
                    <settings.fromName>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.from_name</label>
                            <config type="array">
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.fromName>
                    <settings.subject>
                        <TCEforms>
                            <label>LLL:EXT:dmailsubscribe/Resources/Private/Language/locallang.xml:flexforms.subject</label>
                            <config type="array">
                                <type>input</type>
                                <size>30</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.subject>
                </el>
            </email>
        </sDEF>
    </sheets>
</T3DataStructure>
